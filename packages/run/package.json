{
  "name": "@triplex/run",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/triplex-run.cjs.js",
  "module": "dist/triplex-run.esm.js",
  "types": "dist/triplex-run.cjs.d.ts",
  "exports": {
    ".": {
      "import": "./dist/triplex-run.esm.js",
      "default": "./dist/triplex-run.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "scripts": {
    "dev": "ts-node ./src/index.ts"
  },
  "bin": {
    "triplex": "./src/index.ts"
  },
  "dependencies": {
    "@triplex/editor": "workspace:*",
    "@triplex/rpc": "workspace:*",
    "@vitejs/plugin-react": "^2.2.0",
    "vite": "^3.2.4",
    "vite-plugin-glsl": "^0.5.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "preconstruct": {
    "exports": {
      "extra": {
        ".": {
          "import": "./dist/triplex-run.esm.js",
          "default": "./dist/triplex-run.cjs.js"
        }
      }
    }
  }
}
