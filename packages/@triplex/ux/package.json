{
  "name": "@triplex/ux",
  "version": "0.67.9",
  "private": true,
  "description": "Common UI agnostic components for Triplex user experiences.",
  "author": "Michael Dougall (https://twitter.com/itsdouges)",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "module": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./inputs": {
      "types": "./src/inputs/index.tsx",
      "module": "./src/inputs/index.tsx",
      "default": "./src/inputs/index.tsx"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "swc ./src -d ./dist",
    "typedef": "tsc"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@triplex/bridge": "0.67.9",
    "@triplex/lib": "0.67.9",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.6.5",
    "@triplex/server": "0.67.9",
    "@types/react": "^18.0.27",
    "@types/tinycolor2": "^1.4.3",
    "react-compiler-runtime": "workspace:*",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "./inputs": {
        "types": "./dist/inputs/index.d.ts",
        "module": "./dist/inputs/index.js",
        "default": "./dist/inputs/index.js"
      }
    }
  }
}
