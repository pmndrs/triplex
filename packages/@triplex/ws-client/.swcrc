{
  "$schema": "https://json.schemastore.org/swcrc",
  "minify": true,
  "sourceMaps": false,
  "jsc": {
    "minify": {
      "compress": true,
      "mangle": true
    },
    "target": "es2017",
    "parser": {
      "syntax": "typescript"
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    }
  }
}
