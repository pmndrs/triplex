{
  "name": "@triplex/bridge",
  "version": "0.62.0",
  "private": true,
  "description": "The React Three Fiber editor.",
  "license": "GPL-3.0",
  "author": "Michael Dougall (https://twitter.com/itsdouges)",
  "exports": {
    "./host": {
      "default": "./src/host.ts",
      "types": "./src/host.ts"
    },
    "./client": {
      "default": "./src/client.ts",
      "types": "./src/client.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "swc ./src -d ./dist",
    "typedef": "tsc"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.28",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "exports": {
      "./host": {
        "default": "./dist/host.js",
        "types": "./dist/host.d.ts"
      },
      "./client": {
        "default": "./dist/client.js",
        "types": "./dist/client.d.ts"
      }
    }
  }
}
