{
  "name": "@triplex/ws",
  "version": "0.65.0",
  "private": true,
  "description": "The React Three Fiber editor.",
  "author": "Michael Dougall (https://twitter.com/itsdouges)",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "module": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./react": {
      "types": "./src/react.ts",
      "module": "./src/react.ts",
      "default": "./src/react.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "swc ./src -d ./dist",
    "typedef": "tsc"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.28",
    "@triplex/bridge": "0.65.0",
    "@triplex/server": "0.65.0",
    "@types/react": "^18.0.27",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "optionalDependencies": {
    "ws": "^8.12.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "module": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "./react": {
        "types": "./dist/react.d.ts",
        "module": "./dist/react.js",
        "default": "./dist/react.js"
      }
    }
  }
}
