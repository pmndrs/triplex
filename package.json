{
  "name": "triplex_monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "preconstruct build",
    "ci:publish": "pnpm build && pnpm publish --recursive --access=public && changeset tag",
    "ci:version": "changeset version && pnpm install --lockfile-only",
    "dev": "pnpm --filter geometry_example dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "preconstruct fix && preconstruct dev"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@changesets/cli": "^2.26.0",
    "@preconstruct/cli": "^2.3.0",
    "@swc/core": "^1.3.28",
    "@triplex/run": "workspace:*",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.21.0",
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  }
}
