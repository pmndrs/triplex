{
  "name": "triplex_monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm clean && pnpm --filter @triplex/* build && pnpm typedef",
    "ci:publish": "pnpm build && pnpm publish --recursive --access=public && changeset tag",
    "ci:version": "changeset version && pnpm install --lockfile-only",
    "clean:node_modules": "pnpm -r exec rm -rf node_modules",
    "clean": "pnpm -r exec rm -rf dist",
    "dev:faze": "pnpm --filter @example/faze editor",
    "dev:geometry": "pnpm --filter @example/geometry editor",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "eslint ./",
    "test": "vitest",
    "triplex": "pnpm --filter @example/geometry triplex",
    "typedef": "pnpm --filter @triplex/* typedef"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.0",
    "@swc/core": "^1.3.28",
    "@triplex/run": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint": "^8.34.0",
    "jsdom": "^21.1.0",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4",
    "vitest": "^0.29.8"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.28.0"
}
