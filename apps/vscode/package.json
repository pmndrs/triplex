{
  "name": "triplex-vsce",
  "displayName": "Triplex for VS Code [BETA]",
  "version": "0.2.22",
  "private": true,
  "description": "Your visual workspace for React / Three Fiber.",
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "threejs",
    "r3f",
    "react",
    "react-three-fiber",
    "visual-ide"
  ],
  "bugs": {
    "url": "https://github.com/trytriplex/triplex/issues/new",
    "email": "support@triplex.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trytriplex/triplex"
  },
  "license": "SEE LICENSE IN LICENSE",
  "qna": "https://triplex.dev/docs/community/faq",
  "publisher": "trytriplex",
  "main": "./hook-extension.js",
  "files": [
    "!hook-extension.js",
    "dist",
    "index.html",
    "loading.html",
    "static"
  ],
  "scripts": {
    "build": "pnpm codegen:html && cross-env VITE_CJS_IGNORE_WARNING=true vite build --mode vsce && cross-env VITE_CJS_IGNORE_WARNING=true vite build --mode app",
    "codegen:html": "node --import=tsx ./scripts/generate-html.ts",
    "prepack": "node ../../scripts/workspace-run.js ./apply-publish-config.js",
    "pack": "node scripts/pack.js",
    "publish": "cd out && vsce publish --no-dependencies --skip-duplicate",
    "typedef": "tsc -p ./tsconfig.json && tsc -p ./src/app/tsconfig.json"
  },
  "contributes": {
    "commands": [
      {
        "category": "Triplex",
        "command": "triplex.start",
        "title": "Open File"
      },
      {
        "category": "Triplex",
        "command": "triplex.set-camera-editor",
        "title": "Editor Camera"
      },
      {
        "category": "Triplex",
        "command": "triplex.set-camera-default",
        "title": "Default Camera"
      },
      {
        "category": "Triplex",
        "command": "triplex.element-delete",
        "title": "Delete"
      },
      {
        "category": "Triplex",
        "command": "triplex.element-duplicate",
        "title": "Duplicate"
      },
      {
        "category": "Triplex",
        "command": "triplex.element-jump-to",
        "title": "Zoom To Element"
      },
      {
        "category": "Triplex",
        "command": "triplex.open-file",
        "title": "Go To Definition"
      },
      {
        "category": "Triplex",
        "command": "triplex.refresh-scene",
        "title": "Refresh Scene"
      },
      {
        "category": "Triplex",
        "command": "triplex.reload-scene",
        "title": "Reload Scene"
      }
    ],
    "customEditors": [
      {
        "viewType": "triplex.editor",
        "displayName": "Triplex",
        "selector": [
          {
            "filenamePattern": "*.(tsx|jsx)"
          }
        ],
        "priority": "option"
      }
    ],
    "keybindings": [
      {
        "command": "triplex.element-delete",
        "key": "Backspace",
        "when": "!inputFocus && activeCustomEditorId == 'triplex.editor'"
      },
      {
        "command": "triplex.element-duplicate",
        "key": "Ctrl+D",
        "mac": "Cmd+D",
        "when": "!inputFocus && activeCustomEditorId == 'triplex.editor'"
      },
      {
        "command": "triplex.element-jump-to",
        "key": "F",
        "when": "!inputFocus && activeCustomEditorId == 'triplex.editor'"
      },
      {
        "command": "triplex.refresh-scene",
        "key": "Ctrl+R",
        "mac": "Cmd+R",
        "when": "!inputFocus && activeCustomEditorId == 'triplex.editor'"
      },
      {
        "command": "triplex.reload-scene",
        "key": "Ctrl+Shift+R",
        "mac": "Cmd+Shift+R",
        "when": "!inputFocus && activeCustomEditorId == 'triplex.editor'"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "triplex.element-delete",
          "when": "false"
        },
        {
          "command": "triplex.element-duplicate",
          "when": "false"
        },
        {
          "command": "triplex.element-jump-to",
          "when": "false"
        },
        {
          "command": "triplex.open-file",
          "when": "false"
        },
        {
          "command": "triplex.set-camera-editor",
          "when": "activeCustomEditorId == 'triplex.editor'"
        },
        {
          "command": "triplex.set-camera-default",
          "when": "activeCustomEditorId == 'triplex.editor'"
        },
        {
          "command": "triplex.refresh-scene",
          "when": "activeCustomEditorId == 'triplex.editor'"
        },
        {
          "command": "triplex.reload-scene",
          "when": "activeCustomEditorId == 'triplex.editor'"
        },
        {
          "command": "triplex.start",
          "when": "(resourceExtname === .jsx || resourceExtname === .tsx) && activeCustomEditorId != 'triplex.editor'"
        }
      ],
      "webview/context": [
        {
          "command": "triplex.set-camera-editor",
          "when": "webviewSection == 'play-camera'",
          "group": "camera@1"
        },
        {
          "command": "triplex.set-camera-default",
          "when": "webviewSection == 'play-camera'",
          "group": "camera@2"
        },
        {
          "command": "triplex.element-jump-to",
          "when": "webviewSection == 'element-actions'",
          "group": "1_navigation"
        },
        {
          "command": "triplex.open-file",
          "when": "webviewSection == 'element-actions'",
          "group": "1_navigation"
        },
        {
          "command": "triplex.element-duplicate",
          "when": "webviewSection == 'element-actions'",
          "group": "2_modification"
        },
        {
          "command": "triplex.element-delete",
          "when": "webviewSection == 'element-actions'",
          "group": "99_danger"
        }
      ]
    }
  },
  "activationEvents": [
    "workspaceContains:**/*.{jsx,tsx}",
    "onStartupFinished"
  ],
  "dependencies": {
    "@triplex/client": "0.70.20",
    "@triplex/server": "0.71.9"
  },
  "devDependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.2.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@sentry/node": "^8.30.0",
    "@sentry/react": "^8.30.0",
    "@supabase/supabase-js": "^2.49.1",
    "@triplex/bridge": "0.70.4",
    "@triplex/lib": "0.69.12",
    "@triplex/ux": "0.69.17",
    "@triplex/websocks-client": "0.0.5",
    "@types/raf-schd": "^4.0.3",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/vscode": "^1.88.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vscode/vsce": "^2.26.0",
    "anymatch": "^3.1.3",
    "autoprefixer": "^10.4.16",
    "babel-plugin-react-compiler": "19.0.0-beta-27714ef-20250124",
    "bind-event-listener": "^3.0.0",
    "electron": "34.3.0",
    "electron-builder": "^25.1.3",
    "get-port": "^7.0.0",
    "raf-schd": "^4.0.3",
    "react": "^19.0.0",
    "react-compiler-runtime": "19.0.0-beta-27714ef-20250124",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^3.1.4",
    "suspend-react": "^0.1.3",
    "tailwindcss": "^3.3.2",
    "tiny-invariant": "^1.3.3",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "use-callback-ref": "^1.3.1",
    "vite": "^6.0.7",
    "zustand": "^4.3.2"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "publishConfig": {
    "main": "./dist/extension.js"
  },
  "icon": "./static/logo.png",
  "preview": true,
  "pricing": "Free",
  "sponsor": {
    "url": "https://github.com/sponsors/itsdouges"
  }
}
